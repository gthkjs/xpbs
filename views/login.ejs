<!-- @format -->

<h3>/login</h3>
<form action="/login" method="post">
	<div>
		<label for="email">Email</label>
		<input type="email" name="email" id="email" />
	</div>
	<div>
		<label for="password">Password</label>
		<input type="password" name="password" id="password" />
	</div>
	<input type="submit" value="Login" />
</form>
